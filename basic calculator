a = int(input(" enter the first value: "))
b = int(input(" enter the second value: "))
c = input(" enter the operator: ")
# operator that can be used here is  +,-,/,*,**
if c == '+' :
    print(a + b)
if c == '-' :
    print(a - b)
if c == '/' :
    print(a / b)
if c == '*' :
    print(a * b)
if c == '**' :
    print(a ** b)
